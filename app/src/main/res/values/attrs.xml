<resources>


    <declare-styleable name="HelpItemShowAndHideLayoutSyle">
        <attr name="txt_title" format="string"></attr>
        <attr name="txt_info" format="string"></attr>
        <attr name="img_left" format="reference"></attr>
        <attr name="img_right" format="reference"></attr>
    </declare-styleable>


    <!--评分-->
    <declare-styleable name="RatingBar">
        <attr name="starImageSize" format="dimension" />
        <attr name="starImageWidth" format="dimension" />
        <attr name="starImageHeight" format="dimension" />
        <attr name="starImagePadding" format="dimension" />
        <attr name="starCount" format="integer" />
